{% assign offset = 'now' | date: '%s' %}
{% assign summerSwitch = 'now' | date: '%Y-04-01' | date: '%s' %}
{% for day in (1..31) %}
{% assign summerSwitch = summerSwitch | minus: 86400 %}
{% assign weekday = summerSwitch | date: '%w' %}
{% if weekday == '0' %}
{% break %}
{% endif %}
{% endfor %}
{% assign winterSwitch = 'now' | date: '%Y-11-01' | date: '%s' %}
{% for day in (1..31) %}
{% assign winterSwitch = winterSwitch | minus: 86400 %}
{% assign weekday = winterSwitch | date: '%w' %}
{% if weekday == '0' %}
{% break %}
{% endif %}
{% endfor %}
{% assign today = 'now' | date: '%s' | plus: 0 %}
{% if today < summerSwitch or today > winterSwitch %}
{% assign offset = 0 %}
{% else %}
{% assign offset = 1 %}
{% endif %}
{{ 3600 | times: offset | plus: today | date: '%d %b %Y, %H:%M' }}
</span>
</div>

